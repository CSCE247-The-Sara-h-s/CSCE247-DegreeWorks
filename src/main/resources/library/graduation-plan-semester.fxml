<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="degreesmart.view.GraduationPlanSemesterController" fx:id="graduationPlanSemester" styleClass="semester">
    <children>
        <GridPane maxHeight="Infinity" VBox.vgrow="ALWAYS" style="-fx-background-color: white">
            <columnConstraints>
                <ColumnConstraints minWidth="25" maxWidth="25" />
            </columnConstraints>
            <children>
                <ImageView preserveRatio="true" fitWidth="20" fitHeight="20" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="1" GridPane.rowSpan="1" fx:id="expandSemester">
                    <Image url="@../images/downarrow.png"/>
                </ImageView>
                <Label alignment="CENTER_LEFT" fx:id="semesterName" GridPane.columnIndex="1" GridPane.rowIndex="0" GridPane.columnSpan="5" GridPane.rowSpan="1"/>
                <HBox fx:id="spacer" minHeight="10" maxHeight="10" GridPane.columnIndex="0" GridPane.rowIndex="2" GridPane.columnSpan="1" GridPane.rowSpan="1"/>
            </children>
        </GridPane>
        <VBox fx:id="semesterDetails" maxHeight="Infinity" VBox.vgrow="NEVER" HBox.hgrow="ALWAYS" style="-fx-background-color: white"/>
    </children>
</VBox>
