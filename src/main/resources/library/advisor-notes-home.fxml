<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ListView?>
<?import java.net.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="degreesmart.view.AdvisorNotesHomeController">
    <children>
        <fx:include fx:id="headerPane" source="header-pane.fxml"/>
        <Label text="Search for a Student:" minHeight="16" minWidth="69"/>
   </children>
   <HBox>
        <TextField fx:id="myTextField" promptText="Enter student name..." />
        <Button mnemonicParsing="false" prefHeight="20.0" prefWidth="75.0" text="Search" onMouseClicked="#search" />
   </HBox>
   <ListView fx:id="listView">

   </ListView>
    <children>
        <fx:include fx:id="footerPane" source="footer-pane.fxml"/>
   </children>
    <stylesheets>
      <URL value = "@../styles/student-list.css"/>
   </stylesheets>
</VBox>