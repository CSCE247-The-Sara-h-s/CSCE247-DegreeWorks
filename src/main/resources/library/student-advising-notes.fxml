<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="degreesmart.view.StudentAdvisingNoteController">
    <children>
        <fx:include fx:id="headerPane" source="header-pane.fxml" />
        <GridPane fx:id="courseManagement" maxHeight="Infinity" VBox.vgrow="ALWAYS">
            <columnConstraints>
                <ColumnConstraints percentWidth="20.0" />
                <ColumnConstraints percentWidth="20.0" />
                <ColumnConstraints percentWidth="60.0" />
            <ColumnConstraints />
            <ColumnConstraints />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints percentHeight="1" /> 
                <RowConstraints percentHeight="1" /> 
                <RowConstraints percentHeight="5" />
                <RowConstraints percentHeight="93" />
            </rowConstraints>
            <TableView fx:id="sanTable" GridPane.columnIndex="0" GridPane.columnSpan="5" GridPane.rowIndex="3">
            </TableView>
        </GridPane>
        <fx:include fx:id="footerPane" source="footer-pane.fxml" />
    </children>
    <stylesheets>
        <URL value="@../styles/student-list.css" />
    </stylesheets>
</VBox>
